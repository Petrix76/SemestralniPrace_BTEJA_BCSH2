<Window x:Class="Semestralka.ConsoleReadDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Semestralka"
        mc:Ignorable="d"
        Title="ConsoleRead" Height="186" Width="315">
    <Grid Background="#FFAE2121">
        <TextBox x:Name="ConsoleReadBox" TextWrapping="Wrap" Margin="10,35,10,80" Background="White"/>
        <Button Content="Ok" Margin="0,0,17,10" Click="OkButtonClicked" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="62"/>
    </Grid>
</Window>
